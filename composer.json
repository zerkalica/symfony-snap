{
    "require": {
        "twitter/bootstrap": "master",
        "EHER/PHPUnit": "master",
        "symfony/symfony": "master",
        "doctrine/orm": "master",
        "doctrine/doctrine-bundle": "master",
        "twig/extensions": "master",
        "symfony/assetic-bundle": "master",
        "symfony/swiftmailer-bundle": "master",
        "symfony/monolog-bundle": "master",
        "sensio/distribution-bundle": "master",
        "sensio/generator-bundle": "master",
        "sensio/framework-extra-bundle": "master",
        "jms/security-extra-bundle": "master",
        "jms/di-extra-bundle": "master",
        "knplabs/knp-paginator-bundle": "master",
        "knplabs/knp-menu-bundle": "master",
        "craue/formflow-bundle": "master",
        "symfony/http-kernel": "master",
        "doctrine/doctrine-fixtures-bundle": "master",
        "stof/doctrine-extensions-bundle": "master",
        "doctrine/doctrine-migrations-bundle": "master",
        "symfony/twig-bundle": "master",
        "friendsofsymfony/user-bundle": "master",
        "friendsofsymfony/rest-bundle": "master",
        "liip/imagine-bundle": "master",
        "zerkalica/PHP_CodeSniffer": "master",
        "phpmd/phpmd": "master",
        "sensio/buzz-bundle": "master",
        "jms/debugging-bundle": "master",
        "zerkalica/millwright-boilerplate-bundle": "master",
        "zerkalica/millwright-menu-bundle": "master",
        "zerkalica/millwright-twitter-bootstrap-bundle": "master",
        "zerkalica/millwright-rad-bundle": "master",
        "andreychernykh/lexik-form-filter-bundle": "master",
        "gedmo/doctrine-extensions": "master",
        "elao/web-profiler-extra-bundle" : "dev-master",
        "problematic/acl-manager-bundle": "dev-master",
        "bcc/extra-tools-bundle": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "problematic/acl-manager-bundle",
                "autoload": {
                    "psr-0": { "Problematic\\AclManagerBundle": "" }
                },
                "target-dir": "Problematic/AclManagerBundle",
                "source": {
                    "url": "https://github.com/Problematic/ProblematicAclManagerBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },

        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "andreychernykh/lexik-form-filter-bundle",
                "target-dir": "Lexik/Bundle/FormFilterBundle",
                "autoload":     {
                    "psr-0": { "Lexik\\Bundle\\FormFilterBundle": "" }
                },
                "source": {
                    "url": "https://github.com/AndreyChernykh/LexikFormFilterBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },

        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "zerkalica/symfony2-coding-standart",
                "source": {
                    "url": "https://github.com/zerkalica/Symfony2-coding-standard.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
